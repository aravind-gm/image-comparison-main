# Railway Environment Variables Template
# Copy to .env file for local testing
# Railway will auto-set PORT environment variable

# Backend Configuration
FLASK_ENV=production
FLASK_DEBUG=False
PORT=5000

# Model Configuration
MODEL_NAME=MobileNetV2
MODEL_DEVICE=cpu

# CORS Configuration
CORS_ORIGINS=*

# Performance Tuning
MAX_WORKERS=1
MAX_THREADS=2
REQUEST_TIMEOUT=300
MAX_REQUESTS_PER_WORKER=1000

# Logging
LOG_LEVEL=INFO
